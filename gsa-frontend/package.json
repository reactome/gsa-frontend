{
  "name": "gsa-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "watch": "rm -rf dist; npm-run-all --parallel watch-lib:* serve",
    "serve": "wait-on dist/reactome-gsa-form/public-api.d.ts && ng serve --proxy-config src/proxy.conf.json",
    "start-verbose": "ng serve --verbose",
    "build": "ng build",
    "test": "ng test",
    "build-lib:table": "ng build reactome-table",
    "watch-lib:table": "ng build reactome-table --watch",
    "build-lib:gsa-form": "wait-on dist/reactome-table/public-api.d.ts && ng build reactome-gsa-form",
    "watch-lib:gsa-form": "wait-on dist/reactome-table/public-api.d.ts && ng build reactome-gsa-form --watch",
    "yalc:table": "rm -rf dist/reactome-table && npm run build-lib:table && yalc push dist/reactome-table",
    "yalc:gsa-form": "rm -rf dist/reactome-gsa-form && npm run yalc:table && npm run build-lib:gsa-form && yalc push dist/reactome-gsa-form",
    "build-table-wc": "npm run build-lib:table && ng build reactome-table-wc --output-hashing none && node projects/reactome-table-wc/build-wc.js",
    "build-libs": "npm-run-all --parallel build-lib:* && wait-on dist/reactome-gsa-form/public-api.d.ts",
    "deploy-gh": "npm run build-libs && ng deploy --base-href=/gsa/",
    "deploy-dev": "npm run build-libs && ng build --configuration=dev --base-href=/gsa/ && rsync --chown=www-data:reactome --chmod=775 -rpIgoDvhP dist/gsa/browser/ dev:site/gsa --delete",
    "deploy-release": "npm run build-libs && ng build --configuration=release --base-href=/gsa/ && rsync --chown=www-data:reactome --chmod=775 -rpIgoDvhP dist/gsa/browser/ release:site/gsa --delete",
    "deploy-prod": "npm run build-libs && ng build --configuration=production --base-href=/gsa/ && rsync --chown=www-data:reactome --chmod=775 -rpIgoDvhP dist/gsa/browser/ prod:site/gsa --delete"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/elements": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/component": "^19.2.1",
    "@ngrx/component-store": "^19.2.1",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/entity": "^19.2.1",
    "@ngrx/operators": "^19.2.1",
    "@ngrx/router-store": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "bootstrap": "^5.2.2",
    "cors": "^2.8.5",
    "immer": "^10.0.3",
    "ngrx-wieder": "^14.0.0",
    "ngx-reactome-style": "file:.yalc/ngx-reactome-style",
    "ngx-ui-tour-md-menu": "^14.0.1",
    "rxjs": "~7.5.0",
    "scroll-shadow-element": "^2.0.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "~19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@ngrx/schematics": "^19.2.1",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^18.16.0",
    "angular-cli-ghpages": "^1.0.5",
    "concat": "^1.0.3",
    "fs-extra": "^11.1.1",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^19.2.2",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.3",
    "wait-on": "^7.2.0"
  }
}
